package lesson1;

public class test1 {
	int ages;
	public void setAge(int age) {
		ages = age;
	}
	
	public int getAge() {
		return ages;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		test1 dd = new test1();
		dd.setAge(21);
		
		System.out.println(dd.ages);
	}

}

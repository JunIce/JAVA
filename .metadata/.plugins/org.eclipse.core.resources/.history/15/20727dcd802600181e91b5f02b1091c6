package lesson1;
public class test1 {
	int age;
	public void setAge(int age) {
		age = age;
	}
	
	public int getAge() {
		return age;
	}
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
		test1 dd = new test1();
		dd.setAge(21);
		
		System.out.println(dd.age);
	}

}

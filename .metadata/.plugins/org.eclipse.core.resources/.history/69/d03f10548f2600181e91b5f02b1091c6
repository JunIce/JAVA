package lesson1;
import java.io.*;
public class test {

	public static void main(String args[]) {
		int hexa = 0x64;
		Dog dog = new Dog();
		dog.eat();
		System.out.println(hexa);
		System.out.println(Dog.Name);
		System.out.println(Dog.DEPARTMENT);
		
		int x = 10;
		do {
			System.out.println("value is : "+ x);
			x++;
		}while( x < 20);
		
		int []numbers = {10,20,30,40,50};
		for(int y: numbers) {
			System.out.print(y);
		}
	}
}
